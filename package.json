{
  "name": "react-list-picker",
  "version": "2.5.2",
  "typings": "typings/index.d.ts",
  "description": "This project provides a List Picker Form field for Material-UI and React-Final-Form. The purpose is to have a compelling dymanic way for a user to select input in a form.",
  "main": "lib/ListPicker.js",
  "files": [
    "lib/**/*.js",
    "typings/*.d.ts",
    "README.md",
    "LICENSE",
    "demo.gif"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "test": "standard && jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:unit": "jest",
    "lint": "standard",
    "prepublish": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eylon-sargon/react-list-picker"
  },
  "keywords": [
    "react",
    "material",
    "final-form",
    "input",
    "select",
    "list"
  ],
  "author": "raymaen007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eylon-sargon/react-list-picker/issues"
  },
  "homepage": "https://github.com/Eylon-sargon/react-list-picker#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.5.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.2.2",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "gh-pages": "^1.2.0",
    "jest": "24.9.0",
    "material-ui-chip-input": "^2.0.0-beta.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1",
    "react-test-renderer": "^16.2.0",
    "standard": "^13.1.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^2.0.0 || ^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "final-form": "^3.0.0 || ^4.0.0",
    "final-form-arrays": "^2.0.0 || ^3.0.0",
    "react-final-form": "^5.0.0 || ^6.0.0",
    "react-final-form-arrays": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "material-ui-chip-input": "^2.0.0-beta.1",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test"
    ],
    "roots": [
      "src",
      "test"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
